{
  "name": "dese-ea-plan-v5",
  "version": "6.8.1",
  "description": "CPT Optimization Domain iÃ§in Kubernetes + GitOps + AIOps uyumlu kurumsal planlama",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "pnpm@8.15.0",
  "bin": {
    "master-control": "./dist/cli/masterControl.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:3001": "tsx watch src/index.ts",
    "dev:ops": "tsx watch src/ops-server.ts",
    "build": "tsc && tsc-alias",
    "start": "tsx src/index.ts",
    "start:ops": "tsx src/ops-server.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:smart": "vitest run --coverage && pnpm test:update-readme && pnpm test:check-missing && pnpm test:generate-templates && pnpm test:cleanup",
    "test:update-readme": "node scripts/update-test-readme.js",
    "test:check-missing": "node scripts/check-missing-tests.js",
    "test:generate-templates": "node scripts/generate-test-templates.js",
    "test:cleanup": "node scripts/cleanup-temp-files.js",
    "test:auto": "playwright test",
    "test:auto:ui": "playwright test --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "rbac:seed": "tsx src/rbac/seed.ts",
    "audit:test": "vitest run -t audit",
    "privacy:test": "vitest run -t privacy",
    "metrics:test": "pwsh scripts/test-prometheus-metrics.ps1",
    "metrics:grafana": "pwsh scripts/setup-grafana-dashboard.ps1",
    "metrics:validate": "pwsh scripts/validate-realtime-metrics.ps1",
    "metrics:validate:bash": "bash scripts/validate-realtime-metrics.sh",
    "metrics:alerts": "pwsh scripts/setup-alertmanager.ps1",
    "mcp:finbot": "tsx src/mcp/finbot-server.ts",
    "mcp:mubot": "tsx src/mcp/mubot-server.ts",
    "mcp:dese": "tsx src/mcp/dese-server.ts",
    "mcp:observability": "tsx src/mcp/observability-server.ts",
    "mcp:all": "tsx src/mcp/finbot-server.ts & tsx src/mcp/mubot-server.ts & tsx src/mcp/dese-server.ts & tsx src/mcp/observability-server.ts",
    "ai:stage-context": "git diff --staged > .cursor/context.staged.diff && echo staged diff exported",
    "ai:review:staged": "npm run ai:stage-context && cursor chat --context .cursor/context.staged.diff",
    "docker:build": "docker build -t dese-ea-plan-v5 .",
    "docker:run": "docker run -p 3000:3000 dese-ea-plan-v5",
    "k8s:apply": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "helm:install": "helm install dese-ea-plan-v5 ./helm/dese-ea-plan-v5",
    "helm:upgrade": "helm upgrade dese-ea-plan-v5 ./helm/dese-ea-plan-v5",
    "helm:uninstall": "helm uninstall dese-ea-plan-v5",
    "health:check": "pwsh scripts/advanced-health-check.ps1",
    "health:check:verbose": "pwsh scripts/advanced-health-check.ps1 -Verbose",
    "health:monitor": "pwsh scripts/automated-health-monitor.ps1",
    "health:quick": "pwsh quick-health-check.ps1",
    "cicd:setup": "bash scripts/setup-cicd.sh",
    "cicd:sync": "pwsh gitops-sync.ps1",
    "deploy:staging": "kubectl set image deployment/dese-ea-plan-v5 dese-ea-plan-v5=ghcr.io/$REPO/dese-ea-plan-v5:$COMMIT_SHA -n dese-ea-plan-v5-staging",
    "deploy:production": "kubectl set image deployment/dese-ea-plan-v5 dese-ea-plan-v5=ghcr.io/$REPO/dese-ea-plan-v5:$COMMIT_SHA -n dese-ea-plan-v5",
    "watchdog": "bash ops/watchdog.sh",
    "watchdog:win": "powershell -ExecutionPolicy Bypass -File ops/watchdog.ps1",
    "watchdog:cron": "bash ops/watchdog-cron.sh"
  },
  "keywords": [
    "seo",
    "content-generation",
    "kubernetes",
    "gitops",
    "aiops",
    "cpt-optimization",
    "e-e-a-t",
    "core-web-vitals"
  ],
  "author": "CPT Digital Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bull": "^4.16.5",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "http-proxy-middleware": "^3.0.0",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lighthouse": "^13.0.1",
    "mathjs": "^15.1.0",
    "nodemailer": "^7.0.10",
    "openai": "^6.8.1",
    "postgres": "^3.4.7",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.29.1",
    "redis": "^5.9.0",
    "sharp": "^0.34.5",
    "stripe": "^19.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "k6": "^0.0.0",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "testcontainers": "^11.8.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=8.0.0"
  }
}
