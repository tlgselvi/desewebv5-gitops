{
  "name": "dese-ea-plan-v5",
  "version": "5.0.0",
  "description": "CPT Optimization Domain iÃ§in Kubernetes + GitOps + AIOps uyumlu kurumsal planlama",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:ops": "tsx watch src/ops-server.ts",
    "build": "tsc-alias -p tsconfig.json",
    "start": "tsx src/index.ts",
    "start:ops": "tsx src/ops-server.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:smart": "vitest run --coverage && pnpm test:update-readme && pnpm test:check-missing && pnpm test:generate-templates && pnpm test:cleanup",
    "test:update-readme": "node scripts/update-test-readme.js",
    "test:check-missing": "node scripts/check-missing-tests.js",
    "test:generate-templates": "node scripts/generate-test-templates.js",
    "test:cleanup": "node scripts/cleanup-temp-files.js",
    "test:auto": "playwright test",
    "test:auto:ui": "playwright test --ui",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker build -t dese-ea-plan-v5 .",
    "docker:run": "docker run -p 3000:3000 dese-ea-plan-v5",
    "k8s:apply": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "helm:install": "helm install dese-ea-plan-v5 ./helm/dese-ea-plan-v5",
    "helm:upgrade": "helm upgrade dese-ea-plan-v5 ./helm/dese-ea-plan-v5",
    "helm:uninstall": "helm uninstall dese-ea-plan-v5",
    "health:check": "pwsh scripts/advanced-health-check.ps1",
    "health:check:verbose": "pwsh scripts/advanced-health-check.ps1 -Verbose",
    "health:monitor": "pwsh scripts/automated-health-monitor.ps1",
    "health:quick": "pwsh quick-health-check.ps1",
    "cicd:setup": "bash scripts/setup-cicd.sh",
    "cicd:sync": "pwsh gitops-sync.ps1",
    "deploy:staging": "kubectl set image deployment/dese-ea-plan-v5 dese-ea-plan-v5=ghcr.io/$REPO/dese-ea-plan-v5:$COMMIT_SHA -n dese-ea-plan-v5-staging",
    "deploy:production": "kubectl set image deployment/dese-ea-plan-v5 dese-ea-plan-v5=ghcr.io/$REPO/dese-ea-plan-v5:$COMMIT_SHA -n dese-ea-plan-v5",
    "watchdog": "bash ops/watchdog.sh",
    "watchdog:win": "powershell -ExecutionPolicy Bypass -File ops/watchdog.ps1",
    "watchdog:cron": "bash ops/watchdog-cron.sh"
  },
  "keywords": [
    "seo",
    "content-generation",
    "kubernetes",
    "gitops",
    "aiops",
    "cpt-optimization",
    "e-e-a-t",
    "core-web-vitals"
  ],
  "author": "CPT Digital Team",
  "license": "MIT",
