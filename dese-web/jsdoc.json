{
  "name": "CPT AIOps Documentation",
  "version": "5.8.0",
  "description": "Comprehensive documentation for CPT AIOps platform",
  "baseUrl": "/docs",
  "source": {
    "include": [
      "./README.md",
      "./docs/",
      "./app/",
      "./lib/",
      "./components/"
    ],
    "exclude": [
      "node_modules/",
      ".next/",
      "dist/"
    ],
    "includePattern": "\\.(js|jsx|ts|tsx|md)$"
  },
  "opts": {
    "destination": "./docs-site/",
    "recurse": true,
    "template": "node_modules/docdash",
    "theme": "cosmo"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "default": {
      "layoutFile": "templates/layout.tmpl",
      "staticFiles": {
        "include": [
          "./public/"
        ]
      }
    }
  },
  "markdown": {
    "idInHeadings": true
  },
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": false,
    "wrap": true,
    "typedefs": true,
    "removeQuotes": "none",
    "menu": {
      "Home": {
        "href": "index.html"
      },
      "API Reference": {
        "href": "global.html"
      },
      "Components": {
        "href": "components.html"
      },
      "Guides": {
        "href": "guides.html"
      }
    }
  }
}
