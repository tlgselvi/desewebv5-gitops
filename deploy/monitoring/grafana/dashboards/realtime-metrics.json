{
  "dashboard": {
    "id": null,
    "uid": "realtime-metrics",
    "title": "EA Plan Master Control - Realtime Metrics",
    "tags": ["realtime", "websocket", "redis-streams", "dese"],
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 0,
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "type": "graph",
        "title": "WebSocket Connections",
        "targets": [
          {
            "expr": "ws_connections",
            "refId": "A",
            "legendFormat": "Active Connections"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "short",
            "label": "Connections",
            "show": true
          },
          {
            "format": "short",
            "show": true
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 0
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "type": "graph",
        "title": "WebSocket Broadcast Total",
        "targets": [
          {
            "expr": "rate(ws_broadcast_total[5m])",
            "refId": "A",
            "legendFormat": "Broadcasts/sec"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "ops",
            "label": "Rate",
            "show": true
          },
          {
            "format": "short",
            "show": true
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 1
      },
      {
        "id": 3,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "type": "graph",
        "title": "WebSocket Latency (p95 RTT)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ws_latency_seconds_bucket[5m])) by (le))",
            "refId": "A",
            "legendFormat": "p95 Latency"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(ws_latency_seconds_bucket[5m])) by (le))",
            "refId": "B",
            "legendFormat": "p50 Latency"
          }
        ],
        "xaxis": {
          "mode": "time",
          "show": true
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "show": true
          },
          {
            "format": "short",
            "show": true
          }
        ],
        "lines": true,
        "linewidth": 2,
        "fill": 0
      },
      {
        "id": 4,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "type": "table",
        "title": "Redis Stream Consumer Lag",
        "targets": [
          {
            "expr": "stream_consumer_lag",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "stream": 0,
                "group": 1,
                "Value": 2
              },
              "renameByName": {
                "stream": "Stream",
                "group": "Consumer Group",
                "Value": "Lag (messages)"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          },
          "overrides": []
        }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  },
  "overwrite": true
}

