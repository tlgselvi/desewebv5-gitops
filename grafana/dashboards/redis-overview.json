{
  "uid": "redis-overview",
  "title": "Redis Overview",
  "tags": ["redis", "observability"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "style": "dark",
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Memory Usage",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "redis_memory_used_bytes",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "mappings": [
            {
              "type": "special",
              "options": {
                "noData": {
                  "text": "No Data"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "gauge",
      "title": "Cache Hit Ratio",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(redis_keyspace_hits_total[5m])) / (sum(rate(redis_keyspace_hits_total[5m])) + sum(rate(redis_keyspace_misses_total[5m])))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "mappings": [
            {
              "type": "special",
              "options": {
                "noData": {
                  "text": "No Data"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "percentage",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 70 },
              { "color": "green", "value": 85 }
            ]
          },
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "orientation": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Connected Clients",
      "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "redis_connected_clients",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {
              "type": "special",
              "options": {
                "noData": {
                  "text": "No Data"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "orientation": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "timeseries",
      "title": "Commands Processed Rate (ops/sec)",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 6 },
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(redis_commands_processed_total[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "mappings": [
            {
              "type": "special",
              "options": {
                "noData": {
                  "text": "No Data"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": ["max", "mean", "last"],
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "instance",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "Prometheus"
        },
        "definition": "label_values(redis_memory_used_bytes, instance)",
        "refresh": 1,
        "includeAll": true,
        "multi": true,
        "allValue": ".*",
        "current": {
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  }
}

