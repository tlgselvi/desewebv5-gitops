{
  "dashboard": {
    "title": "Predictive Risk & Rollback Dashboard",
    "tags": ["predictive", "resilience", "rollback", "aiops"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Predicted Risk Score Over Time",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aiops_risk_score",
            "legendFormat": "Risk Score",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "min": 0, "max": 1, "label": "Risk (0-1)"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0, "color": "green"},
          {"value": 0.6, "color": "yellow"},
          {"value": 0.8, "color": "red"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.8], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "30s",
          "name": "High Predicted Risk",
          "noDataState": "no_data",
          "notifications": [],
          "state": "alerting"
        }
      },
      {
        "id": 2,
        "title": "Risk Threshold Visualization",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aiops_risk_score",
            "refId": "A"
          }
        ],
        "options": {
          "valueFontSize": "48px",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 0.6, "color": "yellow"},
              {"value": 0.8, "color": "red"}
            ]
          }
        }
      },
      {
        "id": 3,
        "title": "Anomaly Score",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aiops_anomaly_score",
            "legendFormat": "Anomaly Score",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0, "color": "red"},
          {"value": 0.5, "color": "green"}
        ]
      },
      {
        "id": 4,
        "title": "Rollback History",
        "type": "table",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aiops_rollback_executed_total",
            "format": "table",
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true
        }
      },
      {
        "id": 5,
        "title": "Rollback Decision Over Time",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aiops_rollback_decision{decision=\"rollback\"}",
            "legendFormat": "Rollback",
            "refId": "A"
          },
          {
            "expr": "aiops_rollback_decision{decision=\"proceed\"}",
            "legendFormat": "Proceed",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Model Accuracy",
        "type": "singlestat",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aiops_model_accuracy",
            "refId": "A"
          }
        ],
        "options": {
          "valueFontSize": "48px",
          "unit": "percentunit"
        },
        "thresholds": "50,85"
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Rollback Events",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "rgba(255, 96, 96, 1)",
          "limit": 50,
          "query": "aiops_rollback_executed_total",
          "step": "30s",
          "titleFormat": "Rollback",
          "type": "dashboard"
        }
      ]
    },
    "templating": {
      "list": []
    },
    "links": [
      {
        "icon": "external link",
        "tags": [],
        "title": "Runbook",
        "tooltip": "View predictive rollback runbook",
        "type": "link",
        "url": "https://docs.cpt-digital.com/runbook/predictive-rollback"
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    }
  }
}

