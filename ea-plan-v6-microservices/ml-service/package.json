{
  "name": "ml-service",
  "version": "1.0.0",
  "description": "Machine Learning and Anomaly Detection Service for EA Plan v6.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "docker:build": "docker build -t ea-plan-v6/ml-service .",
    "docker:run": "docker run -p 3005:3005 ea-plan-v6/ml-service"
  },
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.0.0",
    "kafkajs": "^2.2.4",
    "redis": "^4.6.10",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "python-shell": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/python-shell": "^4.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "keywords": ["ml", "microservice", "fastify", "anomaly-detection", "tensorflow", "ea-plan-v6"],
  "author": "EA Plan v6.0 Team",
  "license": "MIT"
}
